{
  "name": "web",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -f src/*.d.ts src/*.js ../assets/index.html ../../demo/node_modules/expo-leaflet/assets/index.html",
    "prebuild": "yarn clean && tsc",
    "build": "react-scripts build",
    "postbuild": "gulp build && cp ./dist/index.html ../assets/index.html && cp ./dist/index.html ../../demo/node_modules/expo-leaflet/assets/index.html",
    "preinstall": "mv node_modules_temp node_modules",
    "postinstall": "mv node_modules node_modules_temp",
    "iterate": "mv node_modules_temp node_modules && yarn build && mv node_modules node_modules_temp"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.0",
    "react-measure": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/leaflet": "1.7.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-leaflet": "^2.5.0",
    "@types/react-measure": "^2.0.5",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-inline-source": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "react-scripts": "4.0.3",
    "typescript": "~4.3.2"
  },
  "futureDeps": {
    "@types/react-leaflet-markercluster": "^2.0.0",
    "leaflet.markercluster": "^1.5.0",
    "react-leaflet-markercluster": "^2.0.0"
  }
}
